{
  "name": "flowbit-analytics",
  "private": true,
  "version": "1.0.0",
  "workspaces": [
    "apps/*",
    "services/*",
    "data"
  ],
  "scripts": {
    "dev:api": "pnpm --filter @flowbit/api dev",
    "dev:web": "pnpm --filter @flowbit/web dev",
    "dev:vanna": "pnpm --filter @flowbit/vanna start",
    "dev": "concurrently -k -n api,web -c blue,green \"pnpm dev:api\" \"pnpm dev:web\"",
    "build": "pnpm -r build"
  },
  "devDependencies": {
    "concurrently": "^9.0.0"
  },
  "packageManager": "pnpm@9.12.0"
}
